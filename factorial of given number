# factorial of given num
n = int(input("please enter number:"))

# factorial by recursion method
def fact(n):
  if n == 1:
    return 1
  else:
    return n*fact(n-1)

print("factorial of 5:", fact(n))

# factorial by for loop
n = 6
fact = 1
for i in range(1,n+1):
  fact = fact * i
print("factorial of 6:",fact)
